#include "config/deadeye_version.h"

std::string deadeye::GetDeadeyeVersion() { return "@Deadeye_VERSION@"; }

unsigned deadeye::GetDeadeyeVersionMajor() { return @Deadeye_VERSION_MAJOR@+0; }

unsigned deadeye::GetDeadeyeVersionMinor() { return @Deadeye_VERSION_MINOR@+0; }

unsigned deadeye::GetDeadeyeVersionPatch() { return @Deadeye_VERSION_PATCH@+0; }

unsigned deadeye::GetDeadeyeVersionTweak() { return @Deadeye_VERSION_TWEAK@+0; }

unsigned deadeye::GetDeadeyeCommitsSinceVersionChange() { return @Deadeye_COMMITS_SINCE_VERSION_CHANGE@+0; }
