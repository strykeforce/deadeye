#
# Deadeye configuration override
#

# NetworkTables server
# DEADEYE_NT_SERVER={{ deadeye_nt_server }}

# Admin web server listens on this port
# DEADEYE_ADMIN_PORT=80

# ImageFile capture source upload dir
# DEADEYE_UPLOAD_DIR={{ deadeye_upload }}

# Wait for NT to populate after (re)connection
# DEADEYE_NT_WAIT_MS=500

# Vision daemon logging level: debug, info, warn, error
# SPDLOG_LEVEL={{ deadeye_spdlog_level }}

# Enable fast shutdown (sync and cut the power); comment out for normal
# DEADEYE_FAST_SHUTDOWN=1
