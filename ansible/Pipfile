[[source]]
name = "pypi"
url = "https://pypi.org/simple"
verify_ssl = true

[scripts]
bootstrap = "echo ansible-playbook -i inventory.yaml bootstrap.yaml -e hostname=deadeye-a"
deadeye = "ansible-playbook --vault-password-file=.vaultpw -i inventory.yaml deadeye.yaml"

[dev-packages]

[packages]
ansible = "*"

[requires]
python_version = "3.7"
